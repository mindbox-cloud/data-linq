****************************** Original expression **********************************
Table(Customer).Where(c => Not(c.IsDeleted))

****************************** SQL result *******************************************
DECLARE @tabledirectcrm_Customers TABLE(
    Id int not null,
    IsDeleted bit not null
)

INSERT INTO @tabledirectcrm_Customers(Id, IsDeleted)
    SELECT current.Id, current.IsDeleted
        FROM directcrm.Customers AS current
            WHERE Id = @__id
SELECT * FROM @tabledirectcrm_Customers

****************************** Rewritten expression **********************************
resultSet => resultSet.GetTable("directcrm.Customers").Where(c => Not(c.GetValue("IsDeleted"))).Any()
