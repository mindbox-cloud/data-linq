****************************** Original expression **********************************
Table(Customer).Where(c1 => (c1.AreaId == 10)).Where(c2 => (c2.TempPasswordEmail == "123"))

****************************** SQL result *******************************************
DECLARE @tabledirectcrm_Customers TABLE(
    AreaId int not null,
    Id int not null,
    TempPasswordEmail nvarchar(256) not null
)

INSERT INTO @tabledirectcrm_Customers(AreaId, Id, TempPasswordEmail)
    SELECT current.AreaId, current.Id, current.TempPasswordEmail
        FROM directcrm.Customers AS current
            WHERE Id = @__id
SELECT * FROM @tabledirectcrm_Customers

****************************** Rewritten expression **********************************
resultSet => resultSet.GetTable("directcrm.Customers").Where(c1 => (c1.GetValue("AreaId") == 10)).Where(c2 => (c2.GetValue("TempPasswordEmail") == "123")).Any()
