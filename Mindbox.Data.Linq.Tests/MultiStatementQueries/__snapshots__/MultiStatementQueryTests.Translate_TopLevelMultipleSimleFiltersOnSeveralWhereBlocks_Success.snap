DECLARE @tabledirectcrm_Customers TABLE(
    Id int not null,
    IsDeleted bit not null,
    TempPasswordEmail nvarchar(256) not null
)

INSERT INTO @tabledirectcrm_Customers(Id, IsDeleted, TempPasswordEmail)
    SELECT current.Id, current.IsDeleted, current.TempPasswordEmail
        FROM directcrm.Customers AS current
            WHERE Id = @__id
SELECT * FROM @tabledirectcrm_Customers
